{
    "name": "amethyst/taxonomy",
    "keywords": ["api"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "amethyst/common": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*",
        "friendsofphp/php-cs-fixer": "^2.8",
        "amethyst/foo": "0.1.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/",
            "Amethyst\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "taxonomy",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\TaxonomyServiceProvider"
            ]
        }
    }
}
